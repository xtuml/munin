@startuml
partition "Critical_simple_job" {
    group "Critical_simple_job"
        #green:CSJA;
        :CSJB;
        fork
            :CSJC;
            :CSJD;
            #gold:CSJE,CRITICAL;
        fork again
            :CSJF;
            :CSJG;
            #gold:CSJH,CRITICAL;
        end fork
        :CSJI;
        #red:CSJJ;
    end group
    detach
    package "Unhappy Events" {
      :CSJB_Err;
      kill
      :CSJD_Err;
      kill
      :CSJE_Err;
      kill
      :CSJF_Err;
      kill
      :CSJG_Err;
      kill
      :CSJI_Err;
      kill    
    }
}
@enduml
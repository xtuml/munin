{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "GenerateEventFiles": {
      "type": "string"
    },
    "JobSpecificationLocation": {
      "type": "string"
    },
    "TestFileLocation": {
      "type": "string"
    },
    "TestFileDestination": {
      "type": "string"
    },
    "Tests": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "TestId": {
            "type": "number"
          },
          "TestName": {
            "type": "string"
          },
          "TotalTests": {
            "type": "number"
          },
          "TestFrequency": {
            "type": "string"
          },
          "TestJobSpec": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "TestJobSpecName": {
                  "type": "string"
                },
                "EventDispatchOrder": {
                  "type": "string"
                }
              },
              "required": [
                "TestJobSpecName",
                "EventDispatchOrder"
              ]
            }
          }
        },
        "required": [
          "TestId",
          "TestName",
          "TotalTests",
          "TestFrequency",
          "TestJobSpec"
        ]
      }
    }
  }
}

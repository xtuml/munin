{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "JobSpecName": {
      "type": "string"
    },
    "JobName": {
      "type": "string"
    },
    "EventDefinition": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "EventId": {
            "type": "string"
          },
          "EventName": {
            "type": "string"
          },
          "NodeName": {
            "type": "string"
          },
          "ApplicationName": {
            "type": "string"
          },
          "DispatchDelay": {
            "type": "string"
          },
          "SequenceStart": {
            "type": "string"
          },
          "EventData": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "EventDataType": {
                  "type": "string"
                },
                "DataName": {
                  "type": "string"
                },
                "DataValue": {
                  "type": "string"
                }
              },
              "required": [
                "DataName",
                "DataValue"
              ]
            }
          },
          "PreviousEventId": {
            "type": "string"
          }
        },
        "required": [
          "EventId",
          "EventName",
          "NodeName",
          "ApplicationName",
          "DispatchDelay",
          "SequenceStart",
          "PreviousEventId"
        ]
      }
    }
  }
}
